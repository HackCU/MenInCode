@font-face
  font-family NexaLight
  src url("/fonts/AlexandriaFLF-Bold.ttf")

input
  font-family NexaLight
  font-size 17px
  color rgba(160, 120, 255, 1)
  border 5px solid rgba(255, 255, 255, 1)
  -webkit-box-shadow inset 0 0 8px  rgba(0,100,0,.2),
            0 0 16px rgba(0,100,0,.2)
  -moz-box-shadow inset 0 0 8px  rgba(0,100,0,.2),
            0 0 16px rgba(0,100,0,.2)
  box-shadow inset 0 0 8px  rgba(0,0,0,.2),
            0 0 16px rgba(0,0,0,.2)
  padding 7px
  background rgba(255,255,255,0.5)
  margin 0 0 10px 0

#submit-stuff-container
  background rgba(255, 255, 255, .45)
  width 40vw
  padding 25px
  float left
  margin-left 27.5vw

#submit-stuff-content
  font-family NexaLight
  padding 0px
  color rgba(0,0,0,1)
  font-size 19px
  height 45vh

#side-bar-container
  background-color rgba(0,0,0,0.4)
  height 460px
  width 800px
  float left

#side-bar-content
  padding 15px
  color rgba(214, 174, 255,1)
  font-family NexaLight
  font-size 35px

